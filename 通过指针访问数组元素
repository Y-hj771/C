#include <iostream>
#include <stdlib.h>
using namespace std;
int main()
{
    int arr[9]={1,2,3,4,5,6,7,8,9};
    int* p= arr;
    cout<<*(p+3)<<endl;
    int i;
    cout<<"数组元素为：";
    for(i=0;i<9;i++)
        {
            cout<<*p<<" ";
            p++;
        }
    cout<<endl;
    system("pause");
    return 0;
}
